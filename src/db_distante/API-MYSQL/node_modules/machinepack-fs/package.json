      "write-json",
      "read",
      "read-json",
      "exists",
      "mv",
      "mkdir",
      "ensure-dir",
      "get-home-dirpath",
      "ensure-json"
    ],
    "testsUrl": "https://travis-ci.org/mikermcneil/machinepack-fs"
  }
}

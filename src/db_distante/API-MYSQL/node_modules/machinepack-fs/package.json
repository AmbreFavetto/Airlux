{
  "name": "machinepack-fs",
  "version": "2.3.0",
  "description": "Work with the local filesystem; list files, write files, etc.",
  "scripts": {
    "test": "node ./node_modules/test-machinepack-mocha/bin/testmachinepack-mocha.js"
  },
  "keywords": [
    "machines",
    "machinepack",
    "filesystem"
  ],
  "author": "Mike McNeil",
  "license": "MIT",
  "devDependencies": {
    "mocha": "~1.19.0",
    "test-machinepack-mocha": "^0.2.2"
  },
  "dependencies": {
    "lodash": "^2.4.1",
    "fs-extra": "~0.10.0",
    "walker": "~1.0.6",
    "machine": "^4.0.0",
    "async": "^0.9.0",
    "machinepack-util": "^0.7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mikermcneil/machinepack-fs.git"
  },
  "machinepack": {
    "friendlyName": "Filesystem",
    "machineDir": "machines",
    "machines": [
      "cp",
      "ls",
      "rmrf",
      "template",
